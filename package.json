{
  "name": "artibox",
  "description": "Artibox",
  "repository": "git@github.com:React-Artibox/artibox.git",
  "author": "Rytass",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna run clean && lerna clean",
    "build:clean": "lerna run build:clean --stream --parallel",
    "build": "lerna run build --stream --sort",
    "format": "prettier './**/*.{js,ts,tsx,json,scss,md}' --write",
    "lint:css": "stylelint ./**/*.scss --config .stylelintrc",
    "lint:ts": "eslint . --config .eslintrc --ext .js,.ts,.tsx",
    "lint": "npm run lint:ts && npm run lint:css",
    "test:clean": "lerna run test:clean --stream --parallel",
    "test": "lerna run test --stream",
    "start-storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages --out ./public",
    "commit": "git-cz",
    "release": "npm run build:clean && lerna version && npm run build && lerna publish from-git"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/cli": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/node": "^12.12.22",
    "@types/jest": "^24.0.25",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "autoprefixer": "^9.7.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "commitizen": "^4.0.3",
    "css-loader": "^3.4.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.8.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lerna": "^3.19.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.12.0",
    "rollup": "^1.27.14",
    "rollup-plugin-postcss": "^2.0.3",
    "sass-loader": "^8.0.0",
    "slate": "^0.47.9",
    "slate-react": "^0.22.10",
    "style-loader": "^1.1.2",
    "stylelint": "^12.0.1",
    "stylelint-config-recommended-scss": "^4.1.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.13.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.4"
  }
}
