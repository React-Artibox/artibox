{
  "name": "artibox",
  "description": "Artibox",
  "repository": "git@github.com:React-Artibox/artibox.git",
  "author": "Rytass",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna run clean && lerna clean",
    "build:clean": "lerna run build:clean --stream --parallel",
    "build": "lerna run build --stream --sort",
    "format": "prettier './**/*.{js,ts,tsx,json,scss,md}' --write",
    "lint:css": "stylelint ./**/*.scss --config .stylelintrc",
    "lint:ts": "eslint . --config .eslintrc --ext .js,.ts,.tsx",
    "lint": "npm run lint:ts && npm run lint:css",
    "test:clean": "lerna run test:clean --stream --parallel",
    "test": "lerna run test --stream",
    "start-storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "commit": "git-cz",
    "version": "lerna version --conventional-commits --no-push",
    "publish": "lerna publish --from-git"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@storybook/addon-info": "^5.2.6",
    "@storybook/cli": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@types/jest": "^24.0.22",
    "@types/slate": "^0.47.3",
    "@types/slate-react": "^0.22.6",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "autoprefixer": "^9.7.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "commitizen": "^4.0.3",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lerna": "^3.18.4",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "react": "^16.11.0",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-dom": "^16.11.0",
    "rollup": "^1.26.4",
    "rollup-plugin-postcss": "^2.0.3",
    "sass-loader": "^8.0.0",
    "slate": "^0.47.9",
    "slate-react": "^0.22.10",
    "style-loader": "^1.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.12.1",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2"
  }
}
