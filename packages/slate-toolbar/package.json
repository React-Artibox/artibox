  "description": "",
  "author": "Rytass",
  "homepage": "https://github.com/React-Artibox/artibox#readme",
  "license": "MIT",
  "sideEffects": false,
  "main": "./index.js",
  "module": "./esm/index.js",
  "typings": "./esm/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/React-Artibox/artibox.git"
